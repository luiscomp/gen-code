var { SESSAO_EXPIRADA } = require("../util/codigoResposta");

module.exports = (req, res, next) => {
    if(req.session.isPopulated || req.url === "${urlValidarSessao}") {
        next();
    } else {
        res.json({
            "codigo": SESSAO_EXPIRADA,
            "error": "Sess√£o expirada ou inexistente"
        })
    }
}